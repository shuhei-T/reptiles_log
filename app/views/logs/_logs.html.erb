<%# =============== 排泄 ============================================================== %>
<div class="shit">
  <% if log.shit == 'good' %>
    <div class='good'>
      <i class="fas fa-poop"></i>
    </div>
  <% elsif log.shit == 'bad' %>
    <div class="bad">
      <i class="fas fa-poop"></i>
    </div>
  <% end %>
</div>
<%# =============== 掃除 ============================================================== %>
<div class="crean">
  <% if log.creaning == true %>
    <i class="fas fa-broom"></i>
  <% end %>
</div>
<%# =============== ハンドリング ====================================================== %>
<div class="hand">
  <% if log.handling.present? %>
    <i class="fas fa-hand-holding"></i>
  <% end %>
</div>
<%# =============== 脱皮 ============================================================== %>
<div class="sheding">
  <% if log.sheding == 'shed' %>
    <%= image_tag 'shed.png', class: 'shed', size: '35x35' %>
  <% elsif log.sheding == 'white' %>
    <%= image_tag 'white.png', class: 'white', size: '35x35' %>
  <% end %>  
</div>
<%# =============== 温浴 ============================================================== %>
<div class="bath">
  <% if log.bath.present? %>
    <i class="fas fa-bath"></i>
  <% end %>
</div>
<%# =============== 体重 ============================================================== %>
<% if log.weight.present? %>
  <div class="weight">
    <p><i class="fas fa-weight"></i> <%=  log.weight %>g</p>
  </div>
<% end %>
<%# =============== 体長 ============================================================== %>
<% if log.length.present? %>
  <div class="length">
    <p><i class="fas fa-ruler"></i>   <%=  log.length %>cm</p>
  </div>
<% end %>
<%# =============== 温度 ============================================================== %>
<% if log.temperature.present? %>
  <div class="temperature">
    <p><i class="fas fa-thermometer-half"></i> <%= log.temperature %>℃</p>
  </div>
<% end %>
<%# =============== 温度 ============================================================== %>
<% if log.humidity.present? %>
  <div class="humidity">
    <p><%= image_tag 'humidity.png', size: '20x20' %> <%= log.humidity %>%</p>
  </div>
<% end %>
<%# =============== メモ ============================================================== %>
<% if log.remark.present? %>
  <p class="text-small mt-2 fw-light"><i class="fas fa-pen"></i> <%= log.remark %></p>
<% end %>
<%# =============== 画像 ============================================================== %>
<% if log.images %>
  <div class="course-item">
    <% log.images.each do |image| %>
    <span><%= image_tag image.to_s %></span>
    <% end %>
  </div>
<% end %>