<%# =============== 調子 ================================================================= %>
<% case log.condition %>
<% when'smile' %>
  <div class="smile">
    <i class="fas fa-smile"></i>
  </div>
<% when 'meh' %>
  <div class="meh">
    <i class="fas fa-meh"></i>
  </div>
<% when 'tired' %>
  <div class="tired">
    <i class="fas fa-tired"></i>
  </div>
<% end %>
